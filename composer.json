{
    "name": "virtual-twins/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/virtual-twins/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "5.5 || 6.0 || 7.0 || 8.0 || 9.0 || 8.0.* || 9.0.* || 9.* || 8.*",
        "jenssegers/mongodb": "3.3.* || 3.4.* || 3.5.* || 3.6.* || 3.7.* || 3.9.* || 4.*",
        "laravel/passport": "6.0.* || 7.0.* || 7.4.* || 7.5.* || 8.0 || 9.0 || 10.*"
    },
    "autoload": {
        "psr-4": {
            "VirtualTwins\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "VirtualTwins",
            "email": "it@virtual-twins.com",
            "role": "Support"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "VirtualTwins\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
